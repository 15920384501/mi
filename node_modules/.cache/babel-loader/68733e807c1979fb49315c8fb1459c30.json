              {\r\n                        img:\"https://cdn.cnbj1.fds.api.mi-img.com/mi-mall/bd7349fbb622ef89d3837327be4b3682.jpg\"\r\n                    },\r\n                    {\r\n                        img:require(\"../../assets/img/11.jpg\")\r\n                    },\r\n                    {\r\n                        img:require(\"../../assets/img/22.jpg\")\r\n                    },\r\n                    {\r\n                        img:require(\"../../assets/img/11.jpg\")\r\n                    },\r\n                ] ,\r\n                // num:1,\r\n                goods:{\r\n                    id:6,\r\n                    name:\"小米10 Pro\",\r\n                    price:\"3999\",\r\n                    num:1,\r\n                }\r\n            }\r\n        },\r\n        methods:{\r\n            // getCart(){\r\n            //     // 把数据添加入vuex购物车\r\n            //     this.$store.commit(\"cart/add\",this.goods);\r\n            // }\r\n            //add 直接调用vuex购物车里面add方法\r\n            ...mapMutations([\"add\"])\r\n        },\r\n        components:{\r\n            swiper,\r\n        },\r\n        computed:{\r\n            ...mapState({\r\n                num:state=>state.num\r\n            })\r\n        },\r\n        created(){\r\n            this.$store.state.vanTabbar = false;\r\n        },\r\n        // 生命周期钩子，最后销毁函数\r\n        destroyed() {\r\n            // 当组件被销毁时候，修改导航数据\r\n            this.$store.state.vanTabbar = true;\r\n        },\r\n    }\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n    .panel{\r\n        padding: 15px 15px 0px 15px;\r\n        h1{\r\n            font-size: 24px;\r\n        }\r\n        .content{\r\n            font-size: 14px;\r\n        }\r\n        .price{\r\n            color:#FF6B00;\r\n            font-size: 28px;\r\n        }\r\n    }\r\n    .arrowtop{\r\n        position:fixed;\r\n        top:30px;\r\n        left:30px;\r\n        z-index: 200;\r\n        width: 42px;\r\n        i{\r\n            display: block;\r\n            width: 42px;\r\n            height: 42px;\r\n            position: absolute;\r\n            left: 0px;\r\n            top:0px;\r\n            font-size: 24px;\r\n        }\r\n    }\r\n</style>"],"sourceRoot":"src/components/details"}]}